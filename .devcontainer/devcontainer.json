{
	"name": "Ruby on Rails",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"workspaceFolder": "/var/www",
	"service": "checkparam",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	"extensions": [
		"rebornix.Ruby"
	],
	"forwardPorts": [
		3000
	],
	"remoteEnv": {
		"BUNDLE_APP_CONFIG": null,
		"BUNDLE_CACHE_ALL": "true",
		"BUNDLE_PATH": "${containerWorkspaceFolder}/vendor/bundle",
		"EDITOR": "code --wait"
	}
}
