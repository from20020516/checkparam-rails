<h5>Recent Gearsets</h5>
<div class="my-4">
  <% cards.each do |set| %>
    <a href="<%= gearset_path(set.id) %>" class="card rounded-0 px-auto mx-auto mb-3" style="width: 18rem;">
      <div class="card-body bg-dark">
        <h5 class="card-title"><%= set.job[I18n.locale] %></h5>
        <%= image_tag set.user.auth["extra"]["raw_info"]["profile_image_url_https"], size: "48x48", class: "mx-1 border border-dark" %>
        <%= image_tag "/icons/64/#{set.main}.png", size: "48x48", class: "mx-1 border border-dark" %>
        <%= image_tag "/icons/64/#{set.body}.png", size: "48x48", class: "mx-1 border border-dark" %>
        <h6 class="card-subtitle my-2 text-muted">@<%= set.user.auth["info"]["nickname"] %></h6>
      </div>
    </a>
  <% end %>
</div>